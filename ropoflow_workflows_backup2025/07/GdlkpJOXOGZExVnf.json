        "jsCode": "const d = $json;\n\nconst query = {\n  text: `\n    INSERT INTO public.lost_reasons (\n      \"id\", \"name\", \"locationId\", \"dateAdded\", \"dateUpdated\", \"created_at\", \"updated_at\"\n    ) VALUES (\n      $1, $2, $3, $4, $5, $6, $7\n    )\n    ON CONFLICT (\"id\") DO UPDATE SET\n      \"name\" = EXCLUDED.\"name\",\n      \"locationId\" = EXCLUDED.\"locationId\",\n      \"dateAdded\" = EXCLUDED.\"dateAdded\",\n      \"dateUpdated\" = EXCLUDED.\"dateUpdated\",\n      \"created_at\" = EXCLUDED.\"created_at\",\n      \"updated_at\" = EXCLUDED.\"updated_at\"\n  `,\n  values: [\n    d._id,\n    d.name,\n    d.locationId,\n    d.createdAt,\n    d.updatedAt,\n    d.createdAt,\n    d.updatedAt\n  ]\n};\n\nreturn {\n  text: query.text,\n  values: query.values\n};\n"
